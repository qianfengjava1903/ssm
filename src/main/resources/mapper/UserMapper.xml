<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lfb.dao.UserDao">

    <select id="findAll" resultType="User">
         SELECT
          a.id,
          a.username,
          a.password,
          a.sex,
          a.hobbies,
          a.phone,
          a.email,
          a.address
        FROM
          user AS a

    </select>


</mapper>